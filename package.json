{
  "name": "landing-new-test",
  "version": "1.0.0",
  "description": "Basic Plaisio landing page configuration for building tatilwind & bootstrtap",
  "main": "index.js",
  "scripts": {
    "compile:sass": "sass src/scss:assets/css",
    "watch:sass": "sass --watch src/scss:assets/css",
    "build-tailwind-css": "tailwindcss build -i src/tailwind_style.css -o assets/css/tailwinds.css",
    "watch-tailwind-css": "tailwindcss build -i src/tailwind_style.css -o assets/css/tailwinds.css --watch",
    "build-all": "npm run compile:sass && npm run build-tailwind-css"
  },
  "keywords": [
    "Bootstrap",
    "bootstrap-v5",
    "sass",
    "learn-bootstrap",
    "learn-sass",
    "tailwind",
    "Tailwind"
  ],
  "author": "Vangelis",
  "license": "ISC",
  "devDependencies": {
    "gulp": "^4.0.2",
    "sass": "^1.56.1",
    "tailwindcss": "^3.2.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "autoprefixer": "^10.4.13",
    "bootstrap": "^5.2.2",
    "gulp-rename": "^2.0.0"
  }
}
